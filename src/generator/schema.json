    "Datapoints": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "name": {
              "type": "string"
            },
            "group": {
              "type": "string"
            },
            "id": {
              "type": "integer"
            },
            "type": {
              "type": "string"
            },
            "access": {
              "type": "string"
            },
            "namespace": {
              "type": "string"
            },
            "version": {
              "type": "string"
            }
          },
          "required": [
            "name",
            "group",
            "id",
            "type",
            "access",
            "version"
          ]
        }
      ]
