{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "Enums": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "name": {
              "type": "string"
            },
            "type": {
              "type": "string"
            },
            "values": {
              "type": "array",
              "items": [
                {
                  "type": "string"
                }
              ]
            },
            "autoId": {
              "type": "boolean"
            }
          },
          "required": [
            "name",
            "type",
            "values",
            "autoId"
          ]
        }
      ]
    },
    "Groups": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "name": {
              "type": "string"
            },
            "persistence": {
              "type": "string"
            },
            "baseId": {
              "type": "string"
            },
            "version": {
              "type": "object",
              "properties": {
                "major": {
                  "type": "integer"
                },
                "minor": {
                  "type": "integer"
                },
                "build": {
                  "type": "integer"
                }
              },
              "required": [
                "major",
                "minor",
                "build"
              ]
            }
          },
          "required": [
            "name",
            "persistence",
            "baseId",
            "version"
          ]
